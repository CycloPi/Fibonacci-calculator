func posi():
	var rigi = get_node("RigidBody2D")
	var rijiPO
	var gggX = 0.00
	var gggY = 0.00
	var ggg = Vector2(gggX,gggY)
	rijiPO = rigi.get_pos()
	ggg = rigi.get_pos()
	if ggg.y > 150:
		ggg.y=0
		rigi.set_pos(ggg)
	#print(ggg.y)
	PavionY = ggg.y

	
	
	
